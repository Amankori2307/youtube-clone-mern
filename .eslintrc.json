{
    "env": {
        "es2021": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "overrides": [],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "import/extensions": [
            "error",
            "ignorePackages",
            {
                "js": "always",
                "mjs": "always",
                "jsx": "always"
            }
        ],
        "indent": [
            "error",
            4
        ],
        "semi": [
            "error",
            "always"
        ],
        "linebreak-style": "off",
        "no-console": "off",
        "comma-dangle":"off"
    }
}